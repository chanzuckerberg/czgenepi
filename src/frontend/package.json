{
  "name": "aspen-ts-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://frontend.genepinet.local:8000",
  "scripts": {
    "serve": "npm run build && npm run start",
    "start": "next start",
    "build": "node --max_old_space_size=8192 node_modules/.bin/next build",
    "dev": "node --max_old_space_size=8192 node_modules/.bin/next dev",
    "local-dev": "API_URL=http://backend.genepinet.local:3000 npm run dev --  -p 8000",
    "tests": "jest --config jest/config.js",
    "e2e": "DEBUG=pw:api jest --config jest/e2e.config.js",
    "e2e-staging-from-local": "AWS_PROFILE=genepi-dev FRONTEND_URL=https://staging.genepi.czi.technology DEPLOYMENT_STAGE=staging make e2e",
    "type-check": "tsc --noEmit",
    "lint": "concurrently \"node_modules/.bin/eslint src/**/*.{ts,tsx} --quiet --fix\" \"node_modules/.bin/stylelint --fix '**/*.{js,ts,tsx,css,scss}'\"  \"npm run type-check\"",
    "lint-ci": "concurrently \"node_modules/.bin/eslint src/**/*.{ts,tsx}\" \"node_modules/.bin/stylelint '**/*.{js,ts,tsx,css,scss}'\"  \"npm run type-check\""
  },
